server {
    listen 80;
    server_name ~^(.+)$;

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/lib/sundaya/joulestore-web-app/joulestore-web-app.sock;
        proxy_connect_timeout 480;
        proxy_read_timeout 480;
        proxy_send_timeout 480;
        send_timeout 480;
    }
}