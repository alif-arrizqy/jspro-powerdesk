server {
    listen 80;
    server_name ~^(.+)$;

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/lib/sundaya/jspro-powerdesk/jspro-powerdesk.sock;
        proxy_connect_timeout 480;
        proxy_read_timeout 480;
        proxy_send_timeout 480;
        send_timeout 480;
    }
}